{

  "manifest_version": 2,
  "name": "Twitch Points Farmer",
  "version": "1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },

  "description": "Recolector de puntos de Twitch.",

  "content_scripts": [
    {
      "matches": ["*://*.twitch.tv/*"],
      "js": ["twitchpointsfarmer.js"],
      "run_at": "document_start"
    }
  ]

}